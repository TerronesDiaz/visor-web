{
  "name": "visor-web",
  "version": "1.3.9",
  "description": "Programa para abrir el POS en la web",
  "scripts": {
    "start": "electron main.js",
    "build": "electron-builder --dir .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "instalar": "electron-builder --win"
  },
  "author": "Francisco Javier Terrones DÃ­az",
  "license": "ISC",
  "build": {
    "appId": "com.terronesdiaz.visorweb",
    "publish": [
      {
        "provider": "generic",
        "url": "https://terronescolima.com/lanzamientos/"
      }
    ],
    "files": [
      "main.js",
      "./python-print/**/*",
      "./node_modules/**/*",
      "./apiCajon/**/*",
      "./vistas/**/*",
      "libusb-1.0.dll",
      "package.json",
      "package-lock.json",
      "appURLs.json"
    ],
    "asar": true,
    "asarUnpack": [
      "python-print/**",
      "appURLs.json"
    ]
  },
  "devDependencies": {
    "@electron/rebuild": "^3.3.0",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "electron-rebuild": "^3.2.9",
    "electron-updater": "^6.1.4"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bootstrap": "^5.3.2",
    "cors": "^2.8.5",
    "electron-updater": "^6.1.4",
    "express": "^4.18.2",
    "jimp": "^0.3.5",
    "node-thermal-printer": "^4.4.0",
    "puppeteer": "^21.3.4",
    "sweetalert2": "^11.6.13",
    "winston": "^3.10.0"
  },
  "main": "main.js"
}
