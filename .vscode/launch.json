{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Main Process",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
        "program": "${workspaceFolder}/main.js",
        "args" : ["."],
        "outputCapture": "std",
        "console": "integratedTerminal",
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }
  